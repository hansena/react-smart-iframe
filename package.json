{
  "name": "react-smart-iframe",
  "version": "1.0.0",
  "description": "A React iframe component that can communicate with its parent.",
  "main": "dist/SmartIframe.js",
  "author": "Alex Hansen",
  "bugs": {
    "url": "https://github.com/hansena/react-smart-iframe/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hansena/react-smart-iframe.git"
  },
  "keywords": [
    "react",
    "iframe",
    "communication",
    "comm",
    "message"
  ],
  "license": "MIT",
  "dependencies": {
    "@babel/preset-react": "^7.8.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "prop-types": "^15.7.2",
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "peerDependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "babel src/SmartIframe.js --out-file dist/SmartIframe.js",
    "_build": "react-scripts build",
    "test": "react-scripts test --env=jsdom"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/*.css.js",
      "!src/**/*.story.{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "coverageReporters": [
      "lcov",
      "clover"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
